{
	"compilerOptions": {
		"module": "commonjs",
		"lib": ["dom", "es2017"],
		"strictNullChecks": true,
		"target": "es2017",
		"declaration": true,
		"noEmit": false,
		"newLine": "LF",
		"outDir": "dist",
		"noImplicitAny": true,
		"sourceMap": true,
		"sourceRoot": "src",
		"allowJs": false,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"typeRoots": ["./node_modules/@types", "./typings"]
	}
}
